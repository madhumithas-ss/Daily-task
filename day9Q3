#include <stdio.h>

int main() {
    int num1, num2, temp1, temp2, remainder, gcd, lcm;

    printf("Enter two integers: ");
    scanf("%d %d", &num1, &num2);

    temp1 = num1;
    temp2 = num2;

    // Find GCD using Euclidean algorithm
    while (temp2 != 0) {
        remainder = temp1 % temp2;
        temp1 = temp2;
        temp2 = remainder;
    }
    gcd = temp1;

    // Compute LCM
    lcm = (num1 * num2) / gcd;

    printf("GCD = %d\n", gcd);
    printf("LCM = %d\n", lcm);

    return 0;
}